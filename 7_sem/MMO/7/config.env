# Настройки для лабораторной работы по кластеризации

# Настройки датасета
DATASET_PATH=kaggle_Interests_group.csv
RANDOM_STATE=42

# Настройки PCA
PCA_VARIANCE_THRESHOLD=0.90
PCA_MAX_COMPONENTS=50

# Настройки t-SNE
TSNE_PERPLEXITY=30
TSNE_N_ITER=1000

# Настройки кластеризации
MAX_CLUSTERS=10
DBSCAN_EPS=0.5
DBSCAN_MIN_SAMPLES=5

# Настройки визуализации
FIGURE_SIZE_LARGE=(15, 10)
FIGURE_SIZE_MEDIUM=(12, 8)
FIGURE_SIZE_SMALL=(8, 6)
DPI=100

# Настройки метрик
SILHOUETTE_THRESHOLD=0.3
CALINSKI_THRESHOLD=100
DAVIES_BOULDIN_THRESHOLD=2.0
